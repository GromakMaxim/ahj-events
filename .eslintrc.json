{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "prefer-destructuring": "off",
    "no-plusplus": "off",
    "no-alert": "off",
    "no-new": "off"
  }
}
